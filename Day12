//Get a number from user and then find the sum of the digits in the given number.
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int n;
		int sum=0;
		Scanner sc=new Scanner(System.in);
		n=sc.nextInt();
		while(n>0)
		{
		    int d=n%10;
		    sum=sum+d;
		    n=n/10;
		}
		System.out.println(sum);
}
}
