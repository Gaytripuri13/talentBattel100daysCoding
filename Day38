import java.util.*;
public class Main
{
 public static void main(String[] args) {
 Scanner sc = new Scanner (System.in);
 System.out.println ("Enter a string");
 String str = sc.next ();
 int a[] = new int[256];
 for (int i = 0; i < str.length (); i++)
 a[str.charAt(i)]++;
 System.out.println ("Non repeating characters are: ");
 for (int i = 0; i < 256; i++)
 if (a[i] == 1)
 System.out.print ((char) i + " ");
 }
}
