
 
 //Write a program to identify if the number is even or odd
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner (System.in);
		int n=sc.nextInt();
		if(n%2==0){
		    System.out.println("the number is even");
		}
		
		else
		System.out.println("the number is odd");
	}
}
