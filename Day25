//area of circle

import java.util.*;
class Main{ 
public static void main (String[] args) {
    Scanner sc = new Scanner (System.in);
    System.out.println("enter the radius");
    int r = sc.nextInt();
    double pi = 3.14;
    
     double area = pi*r*r;
     System.out.println("areaof circle : "+area);
        
    }
}
